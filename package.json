  "name": "clonecoding_wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with Vanilla and NodeJS",
  "main": "init.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/C4stleB/CloneCoding_Wetube.git"
  },
  "author": "Chung Jeon",
